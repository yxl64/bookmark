{
	"manifest_version": 2,
	"name": "bookmarks",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com",
		"bookmarks"
	],

	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; http://ajax.googleapis.com; object-src 'self'"
}